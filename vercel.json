{
  "version": 2,
  "builds": [
    {
      "src": "src/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "src/app.js"
    }
  ],
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "0 21 * * *"
    },
    {
      "path": "/api/cron-test",
      "schedule": "0 * * * *"
    }
  ]
} 